{
  "name": "website-marito",
  "private": true,
  "version": "1.1.1",
  "type": "module",
  "main": "nuxt.config.ts",
  "license": "Apache-2.0",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "lint": "eslint .",
    "postinstall": "nuxt prepare",
    "release": "release-it",
    "deploy": "nuxt generate && deployment --dotfiles -d .output/public"
  },
  "files": [
    "app/",
    "components/",
    "layouts/",
    "app.vue",
    "app.config.ts",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "tailwind.config.ts"
  ],
  "dependencies": {
    "@iconify/vue": "^4.1.2",
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint": "^0.5.2",
    "@nuxt/vite-builder": "^3.13.0",
    "@nuxthq/studio": "^2.0.0",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@tailwindcss/typography": "^0.5.13",
    "nuxt-icon": "^0.6.10"
  },
  "devDependencies": {
    "@iconify-json/fa-brands": "^1.1.9",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/image": "^1.7.0",
    "@nuxt/ui": "^2.18.4",
    "@nuxtjs/i18n": "^8.4.0",
    "@types/node": "^20.14.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "nuxt": "^3.13.0",
    "release-it": "^17.3.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.4.5"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
